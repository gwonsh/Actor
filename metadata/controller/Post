                    "                        if(chkExist) return;",
                    "                        var label = getLabel(record.get('id'), record.get('text'));",
                    "                        selectedCon.add(label);",
                    "                    }",
                    "                    if(action == 'del'){",
                    "                        selectedCon.down('#linkedLbl_' + record.get('id')).destroy();",
                    "                        Ext.get(e.target).setStyle('color', 'black');",
